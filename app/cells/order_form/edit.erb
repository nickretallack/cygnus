<%= form_for OrderForm.new, url: order_forms_path(current_user), html: { id: "formcreator_result", class: "button-submit" } do |f| %>
  <%= f.text_field :title, placeholder: "Title for form (only you will see this)" %>
  <%= f.hidden_field :content %>
  <%= f.submit "Save Form", id: "save" %>
<% end %>
<div class = "button-area float"></div>

<br />

<div class = "order-form" form = "formcreator_result">

  <% CONFIG[:order_form_icons].each do |key, value| %>
    <div id = "<%= key %>-template" class = "hide">
      <%= cell(:order_form).(key) %>
    </div>
  <% end %>

  <fieldset class="controls">
    <legend>Add Question to Form:</legend>
    <div id = "button-area"></div>
  </fieldset>

    <!-- Template --
    <div class="item workspace_element" name="A">
      <div class="handle"></div>
      <div class="content">Item A</div>
      <div class="remove">&#10006;</div>
    </div -->
  <div id = "workspace"></div>

</div>